{
    "_version": "1.72.0",
    "sap.app": {
        "id": "test.ui.ui5.shaula",
        "type": "application",
        "i18n": {
            "bundleUrl": "i18n/i18n.properties",
            "supportedLocales": [
                ""
            ],
            "fallbackLocale": ""
        },
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "mainService": {
                "type": "OData",
                "uri": "/company-management",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "resourceRoots": {
            "ui5.shaula": "./resources/ui5/shaula"
        },
        "dependencies": {
            "minUI5Version": "1.136.3",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {},
                "ui5.shaula": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "test.ui.ui5.shaula.i18n.i18n",
                    "supportedLocales": [
                        ""
                    ],
                    "fallbackLocale": ""
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "path": "test.ui.ui5.shaula.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteHomepage",
                    "pattern": "",
                    "target": [
                        "TargetHomepage"
                    ]
                }
            ],
            "targets": {
                "TargetHomepage": {
                    "id": "Homepage",
                    "name": "Homepage",
                    "type": "View",
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false
                }
            }
        },
        "rootView": {
            "viewName": "test.ui.ui5.shaula.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    }
}